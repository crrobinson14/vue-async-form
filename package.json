{
    "name": "vue-async-form",
    "version": "1.0.0",
    "author": "Chad Robinson <crobinson@medialantern.com",
    "private": false,
    "description": "Formik-inspired form handler for VueJS",
    "keywords": [
        "component",
        "Vue.js",
        "forms"
    ],
    "license": "MIT",
    "main": "dist/vue-async-form.cjs.js",
    "files": [
        "dist"
    ],
    "homepage": "https://github.com/crrobinson14/vue-async-form",
    "bugs": "https://github.com/crrobinson14/vue-async-form/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/crrobinson14/vue-async-form.git"
    },
    "scripts": {
        "prepublishOnly": "npm run release",
        "clean": "rm -rf dist .tmp",
        "build": "run-p build:cjs build:dev build:prod",
        "build:cjs": "rollup -c scripts/rollup.config.js --environment BUILD:cjs",
        "build:dev": "rollup -c scripts/rollup.config.js --environment BUILD:dev",
        "build:prod": "rollup -c scripts/rollup.config.js --environment BUILD:prod | uglifyjs -mc warnings=false --comments -o dist/vue-async-form.min.js",
        "build:test": "cross-env NODE_ENV=test webpack --config scripts/webpack.config.unit.js",
        "watch:test": "cross-env NODE_ENV=test webpack -w --config scripts/webpack.config.unit.js",
        "lint": "eslint --fix \"@(src|test|scripts)/**/*.js\"",
        "testem": "testem",
        "testem:ci": "testem ci --launch PhantomJS",
        "test": "npm run test:unit",
        "test:unit": "run-s build:test testem:ci",
        "test:dev": "run-p watch:test testem",
        "release": "run-s lint clean build"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.5",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-power-assert": "^3.0.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-register": "^6.24.1",
        "cross-env": "^5.0.1",
        "css-loader": "^2.1.1",
        "eslint": "^5.16.0",
        "eslint-config-ktsn": "^2.0.1",
        "eslint-plugin-html": "^5.0.3",
        "glob": "^7.1.2",
        "npm-run-all": "^4.0.2",
        "power-assert": "^1.4.4",
        "rollup": "^1.11.3",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-commonjs": "^9.3.4",
        "rollup-plugin-node-resolve": "^4.2.3",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-vue": "^5.0.0",
        "testem": "^2.15.1",
        "uglify-js": "^3.5.11",
        "vue": "^2.6.10",
        "vue-loader": "^15.7.0",
        "webpack": "^4.30.0"
    },
    "dependencies": {}
}
